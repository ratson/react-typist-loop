{
  "name": "react-typist-loop",
  "version": "0.0.3",
  "description": "Typing animations with React.",
  "main": "lib/TypistLoop.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib",
    "prepublish": "run-s clean build test",
    "lint": "eslint --ext js,jsx,md .",
    "test": "ava",
    "posttest": "npm run lint"
  },
  "peerDependencies": {
    "react-typist": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "del-cli": "^1.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-concise": "^0.8.0",
    "eslint-config-concise-esnext": "^0.8.0",
    "eslint-config-concise-react": "^0.8.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "react-typist": "^1.1.1"
  },
  "repository": "ratson/react-typist-loop",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
