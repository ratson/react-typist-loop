{
  "name": "react-typist-loop",
  "version": "0.0.5",
  "description": "Typing animations with React.",
  "main": "src/TypistLoop.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib",
    "prepublish": "run-s clean build test",
    "lint": "eslint --ext js,jsx,md .",
    "test": "jest",
    "posttest": "npm run lint"
  },
  "peerDependencies": {
    "react-typist": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/react": "^16.9.49",
    "@types/react-test-renderer": "^17.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^5.1.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^17.0.1",
    "react-typist": "^2.0.4",
    "typescript": "^4.0.2"
  },
  "repository": "ratson/react-typist-loop",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
