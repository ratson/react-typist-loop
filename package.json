{
  "name": "react-typist-loop",
  "version": "0.0.5",
  "description": "Typing animations with React.",
  "main": "src/TypistLoop.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib",
    "prepublishOnly": "run-s clean build test",
    "lint": "eslint --ext js,jsx,md .",
    "test": "jest",
    "posttest": "npm run lint"
  },
  "peerDependencies": {
    "react-typist": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-typist": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "react-typist": "^2.0.4",
    "typescript": "^4.2.4"
  },
  "repository": "ratson/react-typist-loop",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
